{
    "name": "electron-pipe",
    "version": "1.0.0",
    "description": "Electron Pipe application",
    "main": "main.js",
    "scripts": {
        "pack": "electron-builder --dir",
        "dist": "electron-builder --publish never",
        "start": "electron ."
    },
    "build": {
        "win": {
            "target": [
                {
                    "target": "nsis",
                    "arch": [
                        "x64",
                        "ia32"
                    ]
                }
            ]
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/thesourorange/electron-pipe.git"
    },
    "keywords": [
        "Electron",
        "Petrinet"
    ],
    "author": "GitHub",
    "license": "CC0-1.0",
    "dependencies": {
        "jquery": "^3.5.1",
        "popper.js": "^1.0.1",
        "underscore": "^1.9.1",
        "beardcss": "^2.2.4",
        "roughjs": "^3.1.0",
        "bootstrap": "^4.1.3",
        "@fortawesome/fontawesome-free": "^5.13.0",
        "jszip": "3.6.0"
    },
    "devDependencies": {
        "electron": "^15.0.0",
        "electron-builder": "^22.11.7",
        "node": "^16.9.1"
    },
    "bugs": {
        "url": "https://github.com/thesourorange/electron-pipe/issues"
    },
    "homepage": "https://github.com/thesourorange/electron-pipe#readme"
}
